    "test": "babel-node test",
    "serve": "webpack-dev-server --config example/webpack.config.js"
  },
  "keywords": [
    "react",
    "redux",
    "history",
    "router",
    "enroute",
    "small"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "path-to-regexp": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  }
