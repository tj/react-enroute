{
  "name": "react-enroute",
  "version": "4.0.4",
  "repository": "tj/react-enroute",
  "description": "Small react router",
  "author": "TJ Holowaychuk <tj@tjholowaychuk.com>",
  "license": "MIT",
  "main": "cjs",
  "module": "esm",
  "react-native": "src",
  "sideEffects": false,
  "scripts": {
    "prepack": "yarn test && size-limit",
    "build": "yarn clean && yarn cjs && yarn esm",
    "cjs": "babel --out-dir cjs src",
    "esm": "cross-env ESM=1 babel --out-dir esm src",
    "test": "yarn build && cross-env NODE_ENV=production babel-node test",
    "clean": "rimraf cjs esm",
    "serve": "webpack serve --config example/webpack.config.js"
  },
  "keywords": [
    "react",
    "redux",
    "history",
    "router",
    "enroute",
    "small"
  ],
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@size-limit/preset-small-lib": "^4.6.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "size-limit": "^4.6.0",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "src",
    "esm",
    "cjs",
    "index.d.ts"
  ],
  "size-limit": [
    {
      "path": "esm/index.js",
      "import": "{Router}",
      "limit": "1.8 KB"
    }
  ]
}
