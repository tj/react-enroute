{
  "name": "react-enroute",
  "version": "3.0.1",
  "repository": "tj/react-enroute",
  "description": "Small react router",
  "author": "TJ Holowaychuk <tj@tjholowaychuk.com>",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "license": "MIT",
  "scripts": {
    "prepack": "yarn cjs && yarn esm && yarn test",
    "cjs": "babel --out-dir build index.js",
